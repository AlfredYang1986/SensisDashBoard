	<Service name="APIConcreteProxyDemo" url="http://api.sensis.com.au/20110229/search?" >
		<key factory="BaseKey">
			<arg name="key" value="vxbhkb39hsh4yyubx8yrrgj9" />
			<urlName name="key" delegate="copy(key)" />
		</key>
		<query factory="Route">
			<arg type="String" name="query" value="Pizza" />
			<arg type="String" name="location" value="South Yarra" />
