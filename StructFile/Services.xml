		</query>

		<query factory="JOSN-RPC">
			<arg type="String" name="method" value="application.fetch" />
			<arg type="IntList" name="params" value="1" />
			<arg type="Int" name="id" value="1" />
		</query>

		<query factory="JOSN-RPC">
			<arg type="String" name="method" value="member.fetch" />
			<arg type="StringList" name="params" value="Sensis" />
			<arg type="Int" name="id" value="1" />
		</query>

		<query factory="JOSN-RPC">
			<arg type="String" name="method" value="role.fetch" />
			<arg type="IntList" name="params" value="378" />
			<arg type="Int" name="id" value="1" />
		</query>

		<query factory="JOSN-RPC">
			<arg type="String" name="method" value="service.fetch" />
