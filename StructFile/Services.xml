<Services>
	<Service name="Mashery-Sandbox" url="http://api.sandbox.mashery.com/v2/json-rpc/246?" >
		<key factory="MasheryKey">
			<arg name="key" value="wyb7g8nghygh42z9pg2bve5w" />
			<arg name="secret" value="KJjAMmZXHV" />
			<urlName name="apikey" delegate="copy(key)" />
			<urlName name="sig" delegate="md5(key,secret,time)"/>
		</key>
		<query factory="JOSN-RPC">
			<arg type="String" name="method" value="member.fetch" />
			<arg type="StringList" name="params" value="SAPI" />
